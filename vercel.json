{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/fetchWebsite",
        "dest": "server.js",
        "methods": ["GET"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ]
  }
  